#FizzBuzz with for loops
#starts a 0!
#pass 3 arguments to the function call:
#FizzBuzz(3,5,100)
def FizzBuzz(min,max,total):
	mult = min * max
	listy = [None] * total
	for number in range(total):
		if (number % mult) == 0:
			listy[number] = "FizzBuzz"
		elif (number % min) == 0:
			listy[number] = "Fizz"
		elif (number % max) == 0:
			listy[number] = "Buzz"
		else:
			listy[number] = number
	print(listy)
			

#FizzBuzz another version
#starts at 1!
#pass 3 arguments to the function call:
#FizzBuzz2(3,5,100)
